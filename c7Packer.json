{
    "variables": {
      "PROJECT_ID": "",
      "MACHINE_TYPE": "",
      "SOURCE_IMAGE_FAMILY": "",
      "REGION": "",
      "ZONE": "",
      "SSH_USERNAME": "",
      "SERVICE_ACCOUNT_JSON": "",
      "IMAGE_NAMES_SPACED": "",
      "IMAGE_FAMILY": ""
    },
    "builders": [
      {
          "type": "googlecompute",
          "state_timeout": "15m",
          "project_id": "{{user `PROJECT_ID`}}",
          "machine_type": "{{user `MACHINE_TYPE`}}",
          "source_image_family": "{{user `SOURCE_IMAGE_FAMILY`}}",
          "region": "{{user `REGION`}}",
          "zone": "{{user `ZONE`}}",
          "image_description": "CentOS 7 Machine Image",
          "image_name": "c7-poc-deepika-test",
          "disk_size": 150,
          "disk_type": "pd-ssd",
          "ssh_username": "{{user `SSH_USERNAME`}}",
          "account_file": "{{ user `SERVICE_ACCOUNT_JSON`}}",
          "image_family": "{{ user `IMAGE_FAMILY`}}"
        }
    ],
    "provisioners": [
      {
        "type": "shell",
        "inline": [
          "sleep 30",
          "uname -a",
          "uptime"
        ]
      }
    ]
  }
