{
    "variables": {
      "PROJECT_ID": "",
      "MACHINE_TYPE": "",
      "SOURCE_IMAGE_FAMILY": "",
      "REGION": "",
      "ZONE": "",
      "SSH_USERNAME": "",
      "SERVICE_ACCOUNT_JSON": "",
      "IMAGE_NAMES_SPACED": "",
      "IMAGE_FAMILY": "",
      "IMAGE_DESCRIPTION": "",
      "IMAGE_NAME": ""
    },
    "builders": [
      {
          "type": "googlecompute",
          "state_timeout": "15m",
          "project_id": "{{user `PROJECT_ID`}}",
          "machine_type": "{{user `MACHINE_TYPE`}}",
          "source_image_family": "{{user `SOURCE_IMAGE_FAMILY`}}",
          "region": "{{user `REGION`}}",
          "zone": "{{user `ZONE`}}",
          "image_description": "{{user `IMAGE_DESCRIPTION`}}",
          "image_name": "{{user `IMAGE_NAME`}}",
          "image_family": "{{ user `IMAGE_FAMILY`}}",
          "disk_size": 150,
          "disk_type": "pd-ssd",
          "ssh_username": "{{user `SSH_USERNAME`}}",
          "account_file": "{{ user `SERVICE_ACCOUNT_JSON`}}"
        }
    ],
    "provisioners": [
      {
        "type": "shell",
        "inline": [
          "sleep 30",
          "uname -a",
          "uptime"
        ]
      },
      {
        "type": "file",
        "source": "./buildPlane-x86_64-Ubuntu_16.04.deb",
        "destination": "/tmp/buildPlane-x86_64-Ubuntu_16.04.deb"
      },
      {
        "type": "shell",
        "inline": [
          "sudo dpkg -i /tmp/buildPlane-x86_64-Ubuntu_16.04.deb",
          "sudo /var/opt/jfrog/pipelines/nodeInit/x86_64/Ubuntu_16.04/boot.sh"
        ]
      },
      {
        "type": "shell",
        "script": "basePull.sh",
        "environment_vars": [
          "IMAGE_NAMES_SPACED={{user `IMAGE_NAMES_SPACED`}}"
        ]
      }
    ]
  }
